{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/mint",
      "dest": "api/backend/routes/mint.js"
    },
    {
      "src": "/api/upload",
      "dest": "api/backend/routes/upload.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/server.js"
    }
  ]
}
